############ GENERAL PARAMETERS #################

PidFile=<%= scope.lookupvar('zabbix::agent::pid_file') %>
LogFile=<%= scope.lookupvar('zabbix::agent::log_file') %>
LogFileSize=0
# DebugLevel=3
# SourceIP=
EnableRemoteCommands=<%= scope.lookupvar('zabbix::agent::enable_remote_commands') ? '1' : '0' %>
LogRemoteCommands=<%= scope.lookupvar('zabbix::agent::log_remote_commands') ? '1' : '0' %>

##### Passive checks related

Server=<%= scope.lookupvar('zabbix::agent::servers').join(',') %>
ListenPort=<%= scope.lookupvar('zabbix::agent::listen_port') %>
ListenIP=<%= scope.lookupvar('zabbix::agent::listen_ip').join(',') %>
StartAgents=<%= scope.lookupvar('zabbix::agent::start_agents') %>

##### Active checks related

ServerActive=<%= scope.lookupvar('zabbix::agent::servers').join(',') %>
Hostname=<%= @fqdn %>
# HostnameItem=system.hostname
HostMetadata=<%= scope.lookupvar('zabbix::agent::host_metadata') %>
# HostMetadataItem=
RefreshActiveChecks=<%= scope.lookupvar('zabbix::agent::refresh_active_checks') %>
BufferSend=<%= scope.lookupvar('zabbix::agent::buffer_send') %>
BufferSize=<%= scope.lookupvar('zabbix::agent::buffer_size') %>
MaxLinesPerSecond=<%= scope.lookupvar('zabbix::agent::max_lines_per_second') %>

############ ADVANCED PARAMETERS #################

# Alias=
Timeout=<%= scope.lookupvar('zabbix::agent::timeout') %>
AllowRoot=<%= scope.lookupvar('zabbix::agent::allow_root') ? '1' : '0' %>
Include=<%= scope.lookupvar('zabbix::agent::config_d_dir') %>

####### USER-DEFINED MONITORED PARAMETERS #######

UnsafeUserParameters=<%= scope.lookupvar('zabbix::agent::unsafe_user_parameters') ? '1' : '0' %>
